{
  "name": "Re-enable Copy",
  "description": "Removes copy disablers from certain pages; gives the information's control back to the user.",
  "version": "0.0.1",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.folha.uol.com.br/*"],
      "js": ["main.js"],
      "run_at": "document_end"
    }
  ]
}
